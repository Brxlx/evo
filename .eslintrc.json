{
  "extends": [
    "@rocketseat/eslint-config/next"
  ],
  "plugins": [
    "import",
    "simple-import-sort",
    "react-hooks"
  ],
  "rules": {
    "react-hooks/exhaustive-deps": "error",
    "import/newline-after-import": "error",
    "simple-import-sort/imports": "error",
    // "semi": [
    //   "error",
    //   "always"
    // ],
    "prettier/prettier": [
      "error",
      {
        "semi": true
      }
    ]
  }
}